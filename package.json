{
  "name": "goodrouter",
  "version": "0.0.12",
  "description": "a good router",
  "main": "bin/router.js",
  "types": "bin/router.d.ts",
  "scripts": {
    "test": "npm run lint & npm run spec-all",
    "compile": "tsc",
    "docs": "typedoc --ignoreCompilerErrors --excludePrivate --excludeExternals --readme none --out docs --name 'Good Router'",
    "lint": "tslint 'src/**.{ts,tsx}'",
    "spec": "tape --require 'ts-node/register' './src/polyfill.ts'",
    "spec-all": "npm run spec 'src/**/*.spec.{ts,tsx}'",
    "coverage": "nyc --reporter text-summary --reporter lcov --include 'src/**/*.{ts,tsx}' --exclude 'src/**/*.spec.{ts,tsx}' --extension '.ts' --extension '.tsx' npm run spec-all",
    "prepublish": "npm run compile & npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LuvDaSun/goodrouter.git"
  },
  "author": "Elmer Bulthuis",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/LuvDaSun/goodrouter/issues"
  },
  "homepage": "https://github.com/LuvDaSun/goodrouter",
  "devDependencies": {
    "@types/blue-tape": "^0.1.30",
    "@types/sinon": "^1.16.34",
    "blue-tape": "^1.0.0",
    "core-js": "^2.4.1",
    "nyc": "^10.0.0",
    "sinon": "^1.17.7",
    "state": "^0.2.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "tslib": "^1.5.0"
  }
}